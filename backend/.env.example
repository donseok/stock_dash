# Stock Dashboard Backend Configuration
# Copy this file to .env and fill in your values

# API Keys (optional - mock data will be used if not set)
NEWS_API_KEY=
KIS_APP_KEY=
KIS_APP_SECRET=

# Database (required for production, dev uses mock data)
DATABASE_URL=postgresql+asyncpg://postgres:postgres@localhost:5432/stock_dash
REDIS_URL=redis://localhost:6379

# Debug mode
DEBUG=True
